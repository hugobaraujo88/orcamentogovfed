{
    "name": "pl_ETL_ibge",
    "properties": {
        "activities": [
            {
                "name": "Extract PIB GDP from IBGE",
                "type": "Copy",
                "dependsOn": [],
                "policy": {
                    "timeout": "0.00:11:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "source": {
                        "type": "JsonSource",
                        "storeSettings": {
                            "type": "HttpReadSettings",
                            "requestMethod": "GET"
                        },
                        "formatSettings": {
                            "type": "JsonReadSettings"
                        }
                    },
                    "sink": {
                        "type": "DelimitedTextSink",
                        "storeSettings": {
                            "type": "AzureBlobFSWriteSettings"
                        },
                        "formatSettings": {
                            "type": "DelimitedTextWriteSettings",
                            "quoteAllText": true,
                            "fileExtension": ".csv"
                        }
                    },
                    "enableStaging": false,
                    "translator": {
                        "type": "TabularTranslator",
                        "mappings": [
                            {
                                "source": {
                                    "path": "$['V']"
                                },
                                "sink": {
                                    "name": "Valor"
                                }
                            },
                            {
                                "source": {
                                    "path": "$['D3C']"
                                },
                                "sink": {
                                    "name": "AnoTrimestre"
                                }
                            }
                        ]
                    }
                },
                "inputs": [
                    {
                        "referenceName": "ds_sourcehttp_ibge",
                        "type": "DatasetReference"
                    }
                ],
                "outputs": [
                    {
                        "referenceName": "ds_sinkdatalake_csv_ibge",
                        "type": "DatasetReference"
                    }
                ]
            },
            {
                "name": "Transform PIB data Load to SQL",
                "type": "ExecuteDataFlow",
                "dependsOn": [
                    {
                        "activity": "Extract PIB GDP from IBGE",
                        "dependencyConditions": [
                            "Succeeded"
                        ]
                    }
                ],
                "policy": {
                    "timeout": "0.00:11:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                },
                "userProperties": [],
                "typeProperties": {
                    "dataflow": {
                        "referenceName": "TransformPIB",
                        "type": "DataFlowReference"
                    },
                    "compute": {
                        "coreCount": 8,
                        "computeType": "General"
                    },
                    "traceLevel": "Fine"
                }
            }
        ],
        "folder": {
            "name": "IBGE Data pipeline"
        },
        "annotations": [],
        "lastPublishTime": "2023-10-04T18:54:36Z"
    },
    "type": "Microsoft.DataFactory/factories/pipelines"
}